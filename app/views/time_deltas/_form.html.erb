<%= form_for [@stock, @stock.time_deltas.build], :html => { :class => 'form-inline' } do |f| %>
  <%= f.label :start%>
  <%= f.text_field :start, :class=>"input-small"%>
  <%= f.label :length%>
  <%= f.text_field :length, :class=>"input-small"%>
  <%= f.submit 'submit', :class => 'btn btn-primary'%>
<% end %>
