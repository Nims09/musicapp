<!-- Shows the stock-->
<h1> Stock </h1>
<table> 
	<tr>
		<th>Stock</th>
		<th>Hashtag</th>		
	</tr>
		<tr>
			<td><%= @stock.name %></td>
			<td><%= @stock.hashtag %></td>
		</tr>
</table>

<!-- Shows the Time Detlas-->
<h3>TimeDeltas: </h2>
<%= render @stock.time_deltas %>

<!-- Creates a Time Delta-->
<h3>Add a TimeDelta:</h2>
<%= render "time_deltas/form" %>

<%= link_to 'Back', stocks_path%>
<%= link_to 'Edit', edit_stock_path(@stock)%>